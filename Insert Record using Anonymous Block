DECLARE
d_id Department.DEPARTMENT_ID%type;
d_name Department.DEPARTMENT_NAME%type:='TESTING';
d_loc_id Department.LOCATION_ID%type := 'CHN-102';
BEGIN
SELECT MAX(DEPARTMENT_ID) INTO d_id FROM DEPARTMENT;
d_id := d_id+10;
INSERT INTO DEPARTMENT (DEPARTMENT_ID,DEPARTMENT_NAME,LOCATION_ID)
VALUES(d_id,d_name,d_loc_id);
COMMIT;
END;
